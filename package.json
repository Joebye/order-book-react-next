{
  "name": "scouterz-challenge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "drizzle-kit generate:pg",
    "create-db": "ts-node -r tsconfig-paths/register scripts/setupDB.ts",
    "run-migrations": "ts-node -r tsconfig-paths/register scripts/migrate.ts",
    "seed": "ts-node -r tsconfig-paths/register scripts/seed.ts",
    "bootstrap": "pnpm generate && pnpm create-db && pnpm run-migrations && pnpm seed"
  },
  "dependencies": {
    "dotenv": "^16.4.1",
    "drizzle-orm": "^0.29.3",
    "next": "14.1.0",
    "pg": "^8.11.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/pg": "^8.11.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  }
}
